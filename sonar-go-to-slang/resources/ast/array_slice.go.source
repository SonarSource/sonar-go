package ast

func allLiterals() {
	// Conversions from slice to array pointer
	s := make([]byte, 2, 4)
	s0 := (*[0]byte)(s)      // s0 != nil
	s1 := (*[1]byte)(s[1:])  // &s1[0] == &s[1]
	s2 := (*[2]byte)(s)      // &s2[0] == &s[0]
	s4 := (*[4]byte)(s)      // panics: len([4]byte) > len(s)
	
	var t []string
	t0 := (*[0]string)(t)    // t0 == nil
	t1 := (*[1]string)(t)    // panics: len([1]string) > len(t)

	u := make([]byte, 0)
	u0 := (*[0]byte)(u)       // u0 != nil
}
